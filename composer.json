{
    "name": "ssf/file-copier",
    "description": "A library to copy file from source to target",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiane Akbly",
            "email": "sofiane.akbly@gmail.com",
            "homepage": "https://sofianeakbly.fr",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "sofiane.akbly@gmail.com"
    },
    "require": {
        "symfony/dotenv": "^5.2",
        "league/flysystem": "^2.0",
        "symfony/filesystem": "^5.2",
        "nesbot/carbon": "^2.45",
        "league/flysystem-sftp": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ssf\\Copy\\": "src"
        },
        "files": [],
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "phpdoc": "phpdoc --directory . --target docs --title 'FC documentation' --ignore vendor -vv"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "0.0.2-beta3",
    "time": "2021-03-05",
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}